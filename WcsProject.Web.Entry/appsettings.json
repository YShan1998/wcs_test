{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "AutoInitialize": false,
    "AutoUpdateStructure": false
  },
  "SqlSugar": {
    "IsAutoRemoveDataCache": true,
    "PgSqlIsAutoToLower": false,
    "EnableJsonb": true,
    "LogSqlExecuting": true,
    "LogSqlExecuted": true
  },
  "ConnectionConfigs": [
    {
      "ConfigId": 1,
      "DbType": "PostgreSQL",
      "ConnectionString": "${ConnectionStrings__defaultdb:NOT_SET}",
      "IsAutoCloseConnection": true,
      "InitKeyType": "Attribute"
    }
  ]
}
