{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "ConnectionStrings": {
    "postgres": "${ConnectionStrings__defaultdb:NOT_SET}"
  },
  "ConnectionConfigs": [
    {
      "ConfigId": 1,
      "DbType": "PostgreSQL",
      "ConnectionString": "${ConnectionStrings__defaultdb:NOT_SET}",
      "IsAutoCloseConnection": true,
      "InitKeyType": "Attribute"
    }
  ],
  "SqlSugar": {
    "IsAutoRemoveDataCache": true,
    "PgSqlIsAutoToLower": false,
    "EnableJsonb": true,
    "LogSqlExecuting": true,
    "LogSqlExecuted": true
  }
}
